<header id="application_Header">
<!--must be 100-->
	<div class="fix_header_size_20">
		<h1>Example Jelly</h1>
	</div>
	<div id="app_navigation" class="fix_header_size_50">
		<ul>
			<li><%= link_to "home", user_path(current_user) %></li>
			<li><%= link_to "location", user_path(current_user) %></li>
			<li><%= link_to "food", user_path(current_user) %></li>
			<% if admin_present? %>
				<li><%= link_to "users",  users_path %></li>
			<% end %>
			<% if admin_present? %>
				<li><%= link_to "config", '#' %></li>
			<% end %>
		</ul>
	</div>
	<div class="fix_header_size_20">
		<p><%= current_user.name.downcase %><span> logged in as </span><%= current_user.role.downcase %></p>
	</div>
	<div class="fix_header_size_5">
		<p><%= link_to "profile", user_path(current_user) %></p>
	</div>
	<div class="fix_header_size_5">
		<% if logged_in? %>
			<p><%= link_to "log out", logout_path, method: :delete %></p>
		<% else %>
			<p><%= link_to "log in", login_path %></p>
		<% end %>
	</div>
</header>